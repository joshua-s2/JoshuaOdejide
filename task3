  <script>

    function numberInputTaker(a) {
      let newArray = []
      let divisibleByTwo = 'yu'
      let divisibleByThree = 'gi'
      let divisibleByFive = 'oh'
      for (let i = 1; i <= a; i++) {
        let output = ''
        if (i % 2 === 0) {
          output = divisibleByTwo
        } if (i % 3 === 0) {
          output = divisibleByThree
        } if (i % 5 == 0) {
          output = divisibleByFive
        } if (i % 2 === 0 && i % 3 == 0 && i % 5 == 0) {
          output = divisibleByTwo + '-' + divisibleByThree + '-' + divisibleByFive
        } if (i % 2 == 0 && i % 3 == 0) {
          output = divisibleByTwo + '-' + divisibleByThree
        } if (i % 2 == 0 && i % 5 == 0) {
          output = divisibleByTwo + '-' + divisibleByFive
        } if (i % 3 == 0 && i % 5 == 0) {
          output = divisibleByThree + '-' + divisibleByFive
        }
        newArray.push(output || i)
      }
      console.log(newArray)
    }

    (numberInputTaker(10))
  </script>
</body>

